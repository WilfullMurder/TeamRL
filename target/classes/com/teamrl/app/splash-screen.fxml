        </ImageView>
        <ImageView fitHeight="472.0" fitWidth="672.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@img/mini-logo.png" />
            </image>
        </ImageView>
        <Label layoutX="203.0" layoutY="-12.0" prefHeight="140.0" prefWidth="266.0" text="OCA" textFill="#030000">
            <font>
                <Font name="Goudy Old Style" size="120.0" />
            </font>
        </Label>
        <Label layoutX="263.0" layoutY="392.0" prefHeight="18.0" prefWidth="138.0" text="Your Digital store front" textFill="#fffcfc">
            <font>
                <Font name="Gill Sans MT" size="14.0" />
            </font>
        </Label>
        <Text fill="#fffdfd" layoutX="268.0" layoutY="423.0" strokeType="OUTSIDE" strokeWidth="0.0" text="A style for every story">
